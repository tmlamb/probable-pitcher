    "@probable/tailwind-config": "workspace:*",
    "@probable/tsconfig": "workspace:*",
    "@types/node": "^20.16.11",
    "dotenv-cli": "^7.4.2",
    "eslint": "catalog:",
    "h3": "^1.13.0",
    "nitropack": "^2.9.7",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@probable/prettier-config"
}
